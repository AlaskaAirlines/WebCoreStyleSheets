{
  "name": "@alaskaairux/webcorestylesheets",
  "description": "Auro core foundation Sass UI Kit",
  "version": "3.6.3",
  "author": "Product design and research",
  "main": "src/",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/AlaskaAirlines/WebCoreStyleSheets"
  },
  "peerDependencies": {
    "@alaskaairux/design-tokens": "^3.0.0",
    "sass": "^1.42.1"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "focus-visible": "^5.1.0"
  },
  "devDependencies": {
    "@alaskaairux/design-tokens": "^3.4.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.1.3",
    "auto-changelog": "^2.3.0",
    "concat": "^1.0.3",
    "copyfiles": "^2.4.1",
    "glob": "^7.2.0",
    "husky": "^4.3.8",
    "jest": "^26.4.2",
    "lit-element": "^2.5.1",
    "live-server": "^1.2.1",
    "node-sass": "^6.0.1",
    "nodemon": "^2.0.13",
    "npm-run-all": "^4.1.5",
    "replace-in-file": "^6.2.0",
    "sass": "^1.42.1",
    "sass-lint": "^1.13.1",
    "sass-true": "^5.0.0",
    "sassdoc": "^2.7.3",
    "semantic-release": "^17.4.4",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-standard": "^20.0.0"
  },
  "scripts": {
    "build:sass:demo": "sass --no-source-map ./src/elementDemoStyles/:./dist/",
    "build:sass:test": "sass --no-source-map ./tests/styleTest.scss:./results/css/testBuild.css",
    "build:sass:essentials": "sass --no-source-map ./src/bundled/essentials.scss:./dist/bundled/essentials.css",
    "copyFiles": "copyfiles -u 1 -V './src/**/_*.scss' './src/**/*.js' ./dist",
    "postinstall": "node packageScripts/postinstall.js",
    "copyFontsTest": "copyfiles -f -V './src/fonts/*.*' ./test/fonts",
    "copyFontsDist": "copyfiles -f -V './src/fonts/*.*' ./dist/fonts",
    "cssLint": "stylelint './results/css/*.css'",
    "scssLint": "stylelint './src/**/*.scss'",
    "jest": "jest",
    "test": "npm-run-all scssLint build:sass:test cssLint jest",
    "sassdoc": "sassdoc ./src -d ./docs && node scripts/sassdocTheme.js",
    "serve": "npm run sassdoc && node scripts/live-server.js",
    "devBuild": "npm-run-all scssLint buildCss cssLint jest",
    "watch": "nodemon -e scss --watch tests --watch src --exec npm run devBuild",
    "build": "npm-run-all test build:sass:demo copyFontsDist copyFiles build:sass:test build:sass:essentials postinstall"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release": {
    "branch": "master",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "./CHANGELOG.md",
          "changelogTitle": "# Semantic Release Automated Changelog"
        }
      ],
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "./CHANGELOG.md",
            "./package.json",
            "./docs/index.html"
          ]
        }
      ],
      "@semantic-release/github"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "alaska airlines",
    "auro design system",
    "css",
    "sass"
  ],
  "contributors": [
    {
      "name": "Dale Sande",
      "email": "dale.sande@alaskaair.com"
    },
    {
      "name": "Jason Baker",
      "email": "jason.baker@alaskaair.com"
    },
    {
      "name": "Camron Lockeby",
      "email": "camron.lockeby@alaskaair.com"
    },
    {
      "name": "Kevin Sonnichsen",
      "email": "Kevin.Sonnichsen@alaskaair.com"
    }
  ]
}
